{
  "name": "openai-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for OpenAI API integration - chat completions, embeddings, and more",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "watch": "tsc --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "openai",
    "ai",
    "chatgpt",
    "dall-e",
    "embeddings",
    "model-context-protocol"
  ],
  "author": "Axl Ibiza, MBA",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "openai": "^4.67.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrexibiza/openai-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/andrexibiza/openai-mcp-server/issues"
  },
  "homepage": "https://github.com/andrexibiza/openai-mcp-server#readme"
}